generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  userId String @unique
  userName String @default("")
}

model Warn {
  id Int @default(autoincrement()) @unique
  userId String
  reason String?
}

model BadWord {
  id Int @default(autoincrement()) @unique
  userId String
  message String?
}